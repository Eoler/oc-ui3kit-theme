{
  "name": "oc-ui3kit-theme",
  "version": "1.21.1",
  "description": "Starter development template for building UIkit v3 themed OctoberCMS sites.",
  "author": "Danilo CuculiÄ‡ <eoler@castus.me>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12.22 <=14"
  },
  "main": "gulpfile.js",
  "paths": {
    "root": "./",
    "media": "storage/media/**/",
    "assets": "assets/",
    "src": {
      "images": "images/**/",
      "styles": "scss/",
      "scripts": "es6/"
    },
    "dest": {
      "images": "images/",
      "styles": "css/",
      "scripts": "js/"
    },
    "ext": {
      "images": "*.{jpg,jpeg,png}",
      "styles": "*.scss",
      "scripts": "*.js"
    },
    "include": {
      "sass": [
        "node_modules/uikit/src/scss"
      ],
      "js": [
        "node_modules"
      ]
    },
    "sourcemap": {
      "root": "../scss"
    }
  },
  "images": {
    "maxwidth": "1280",
    "maxheight": "1280"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "iOS >= 10"
  ],
  "dependencies": {
    "modernizr": "^3.11.8",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "postcss-csso": "^5.0.1",
    "uikit": "^3.8.1"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-if": "^3.0.0",
    "gulp-ignore": "^3.0.0",
    "gulp-include": "^2.4.1",
    "gulp-load-plugins": "^2.0.7",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-responsive": "^3.0.1",
    "gulp-sass": "^4.1.1",
    "gulp-size": "^4.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "js-yaml": "^4.1.0",
    "pump": "^3.0.0",
    "yargs": "^17.2.1"
  },
  "scripts": {
    "start": "gulp watch",
    "build": "gulp upbuild --production",
    "modrnize": "modernizr -c modernizr.json -d assets/js"
  }
}
